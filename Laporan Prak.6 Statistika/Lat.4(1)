data_banon=read.csv("C:/Users/asus/Documents/prakstatistika/DirectMarketing.csv")
library(ggplot2)
View(data_banon)
hist(data_banon$AmountSpent)
hist(data_banon$AmountSpent[data_banon$Married=="Single"])
hist(data_banon$AmountSpent[data_banon$Married=="Married"])
d=density(data_banon$AmountSpent)
plot(d)
d=density(data_banon$AmountSpent[data_banon$Married=="Married"])
plot (d)
