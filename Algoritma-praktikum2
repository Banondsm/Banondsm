import math

angka1 = float(input("masukkan angka 1: "))
angka2 = float(input("masukkan angka 2:"))

angka1 = angka1 + angka2
print("hasil angka1 dan angka2 itu adalah? ", angka1)

angka2 = angka1 - angka2
print("selisih angka1 dan angka2 itu adalah? ", angka2)

angka3 = angka1 * angka2
print("perkalian angka1 dan angka2 itu adalah? ", angka3)

angka4 = angka1 / angka2
print("pembagian angka1 dan angka2 itu adalah? ", angka4)

angka5 = angka1 % angka2
print("sisa pembagian angka1 dan angka2 itu adalah? ", angka5)

angka6 = math.log(angka1)
print("logaritma baris 10 adalah",math.log10(angka1))

Number7 = angka1 ** angka2 
print("angka1 pangkat angka2 =", Number7)
